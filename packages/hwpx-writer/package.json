{
  "name": "@handoc/hwpx-writer",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": { "import": "./dist/index.js", "require": "./dist/index.cjs", "types": "./dist/index.d.ts" }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fast-xml-parser": "^4.0.0",
    "@handoc/document-model": "workspace:*",
    "@handoc/hwpx-core": "workspace:*"
  },
  "devDependencies": {
    "vitest": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
