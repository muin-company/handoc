{
  "name": "@handoc/pdf-export",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@handoc/hwpx-parser": "workspace:*",
    "@handoc/document-model": "workspace:*"
  },
  "peerDependencies": {
    "playwright": ">=1.40.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    }
  },
  "devDependencies": {
    "vitest": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "description": "HWPX to PDF export via HTML rendering and Playwright"
}
