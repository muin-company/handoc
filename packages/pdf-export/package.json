{
  "name": "@handoc/pdf-export",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@handoc/document-model": "workspace:*",
    "@handoc/hwpx-parser": "workspace:*",
    "pdf-lib": "^1.17.1"
  },
  "peerDependencies": {
    "playwright": ">=1.40.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.0"
  },
  "description": "HWPX to PDF export via HTML rendering and Playwright",
  "keywords": [
    "hwpx",
    "pdf",
    "export",
    "converter",
    "hancom",
    "document"
  ],
  "author": "MUIN <mj@muin.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muin-company/handoc.git",
    "directory": "packages/pdf-export"
  },
  "homepage": "https://github.com/muin-company/handoc/tree/main/packages/pdf-export",
  "bugs": {
    "url": "https://github.com/muin-company/handoc/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
