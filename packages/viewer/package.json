{
  "name": "@handoc/viewer",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup src/index.ts src/styles.css --format esm,cjs --dts --external react --external react-dom",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@handoc/document-model": "workspace:*",
    "@handoc/hwpx-parser": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.3.0",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "description": "React component for rendering HWPX documents",
  "keywords": [
    "hwpx",
    "viewer",
    "react",
    "component",
    "hancom",
    "document"
  ],
  "author": "MUIN <mj@muin.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muin-company/handoc.git",
    "directory": "packages/viewer"
  },
  "homepage": "https://github.com/muin-company/handoc/tree/main/packages/viewer",
  "bugs": {
    "url": "https://github.com/muin-company/handoc/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
