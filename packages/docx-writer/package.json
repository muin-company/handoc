{
  "name": "@handoc/docx-writer",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@handoc/document-model": "workspace:*",
    "@handoc/hwpx-parser": "workspace:*",
    "docx": "^9.0.0"
  },
  "devDependencies": {
    "fflate": "^0.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.0"
  },
  "description": "Convert HWPX documents to DOCX format",
  "keywords": [
    "hwpx",
    "docx",
    "converter",
    "hancom",
    "document"
  ],
  "author": "MUIN <mj@muin.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muin-company/handoc.git",
    "directory": "packages/docx-writer"
  },
  "homepage": "https://github.com/muin-company/handoc/tree/main/packages/docx-writer",
  "bugs": {
    "url": "https://github.com/muin-company/handoc/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
